{
  "version": 2,
  "name": "quits-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization, X-Gmail-Token, X-User-ID, Origin"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GOOGLE_CLIENT_ID": "your_google_client_id_here",
    "GOOGLE_CALLBACK_URL": "https://api.quits.cc/auth/google/callback",
    "SESSION_SECRET": "your_session_secret_here",
    "SUPABASE_URL": "https://pihflemmavointdxjdsx.supabase.co",
    "CLIENT_URL": "https://quits.cc"
  }
} 